<div class="container">
  <div class="row">
    <div class="main-box">
      
      <h1>Add a New Course</h1>

      <%= simple_form_for @course, url: instructor_courses_path do |f| %>
        <%= f.input :title %>
        <%= f.input :description %>
        <%= f.input :cost %>
        <%= f.input :image %>
        <br />
        <%= f.submit "Create Course", class: "btn btn-pink" %>
        <br />
      <% end %>

    </div>
  </div>
</div>